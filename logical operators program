logical operators program 
 
class Logical 
{ 
   public static void main(String args[]) 
   { 
     //logical AND(&&)     
 System.out.println((6>4)&&(9>6));//true 
     System.out.println((6>4)&&(8<5));//false 
     //Logical OR(||) 
     System.out.println((6>4)||(9>6));//true 
     System.out.println((6>4)||(8<5));//true 
    //logical NOT(!) 
     System.out.println(!(5==3));//true 
     System.out.println(!(5>3));//false 
   } 
} 
